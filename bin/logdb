dbprefix=`date +%Y-%m-%d`
localurl="http://localhost:5984/ssmp_log-"
remoteurl="http://a73434.berlin.ptb.de:5984/ssmp_log-"
replurl=http://localhost:5984/_replicate
echo 'try to generate logdb' 
curl -s -X PUT  $localurl$dbprefix &> /dev/null
echo 'replicate logdb to main'
curl -H 'Content-Type: application/json' -X POST $replurl -d "{\"source\":
\"$localurl$dbprefix\", \"target\": \"$remoteurl$dbprefix\",
\"create_target\": true, \"continuous\": true}" &> /dev/null