{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
    "include": ["lib", "extensions", "."],
	"exclude": [ "test", "node_modules", "bin", ".git", "info", "doc"],
	"includePattern": ".+\\.js(doc)?$",
	"excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
	"systemName"            : "ssmp",
	"footer"                : "server side measurement program",
	"copyright"             : "BSD",
	"includeDate"           : true,
	"navType"               : "vertical",
	"theme"                 : "yeti",
	"linenums"              : true,
	"collapseSymbols"       : false,
	"inverseNav"            : true,
	"outputSourceFiles"     : true ,
	"outputSourcePath"      : true,
	"dateFormat"            : "dddd, MMMM Do YYYY, h:mm:ss",
	"syntaxTheme"           : "default",
	"sort"                  : true
    }
}

