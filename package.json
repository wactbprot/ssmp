{
  "author": "wactbprot",
  "name": "ssmp",
  "description": "server side measurment program",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wactbprot/ssmp"
  },
  "dependencies": {
    "underscore": ">=1.8.3",
    "clone": ">=1.0.2",
    "restify": ">=3.0.3",
    "commander": ">=2.8.1",
    "bunyan": ">=1.3.5",
    "bunyan-couchdb-stream": ">=0.0.1",
    "object-path": ">=0.9.2",
    "socket.io": ">=1.3.5",
    "ndata": ">=2.8.3",
    "handlebars": ">=1.3.0"
  },
  "scripts": {
    "mpshell": "bin/mpsh",
    "doc": "dox-foundation --source ./ --target doc/ --ignore coverage,test,node_modules -t ssmp",
    "server": "node ssmp-server.js | bunyan  -o short",
    "clients": "node ssmp-clients.js | bunyan  -o short",
    "info":"node ./info/info.js | bunyan  -o short",
    "api":"node ./api/api.js | bunyan  -o short",
    "start": "npm run server; npm run clients",
    "all-dev": "npm test && npm run doc",
    "test": "mocha | bunyan -l 100",
    "cover": "istanbul cover _mocha -- -R spec | bunyan -l 100"
  },
  "devDependencies": {
    "dox-foundation": ">=0.5.6",
    "istanbul": ">=0.3.14",
    "look": ">=0.1.3",
    "mocha": ">=2.2.5",
    "prettyjson": ">=1.1.2"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "*"
  }
}
