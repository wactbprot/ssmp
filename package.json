{
  "author": "wactbprot",
  "name": "ssmp",
  "description": "server side measurment program",
  "version": "0.8.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wactbprot/ssmp"
  },
  "dependencies": {
    "underscore": ">=1.8.3",
    "clone": ">=1.0.2",
    "restify": ">=3.0.3",
    "commander": ">=2.9.0",
    "bunyan": ">=1.3.5",
    "bunyan-couchdb-stream": ">=0.0.1",
    "object-path": ">=0.9.2",
    "socket.io": ">=1.3.5",
    "sc-broker": ">=2.1.0",
    "handlebars": ">=4.0.0"
  },
  "scripts": {
    "doc": "jsdoc -c ./jsdoc-conf.json -R README.md  -t ./node_modules/ink-docstrap/template/ -d jsdoc",
    "server": "bin/server | bunyan  -o short",
    "clients": "bin/clients | bunyan  -o short",
    "info":"node ./info/info.js | bunyan  -o short",
    "api":"node ./api/api.js | bunyan  -o short",
    "test": "mocha | bunyan -l 100",
    "cover": "istanbul cover _mocha -- -R spec | bunyan -l 100"
  },
  "devDependencies": {
      "jsdoc": ">=3.4.0",
      "ink-docstrap": ">=1.1.0",
      "istanbul": ">=0.4.0",
      "look": ">=0.1.3",
      "mocha": ">=2.3.0",
      "prettyjson": ">=1.1.2"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "*"
  }
}
